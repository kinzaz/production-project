{
    "name": "production-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --env port=3000",
        "start:vite": "vite",
        "start:dev:server": "node ./json-server/index.js",
        "start:dev": "concurrently \"npm run start\" \"npm run start:dev:server\"",
        "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:dev:server\"",
        "build:prod": "webpack --env mode=production apiUrl=https://vercel.com/kinzaz/production-project-server",
        "build:dev": "webpack --env mode=development",
        "eslint": "eslint \"**/*.{ts,tsx}\"",
        "eslint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
        "prettier": "npx prettier . --write",
        "stylelint": "npx stylelint \"**/*.scss\"",
        "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
        "test:unit": "jest --config ./config/jest/jest.config.ts",
        "test:ui": "npx loki test",
        "test:ui:ok": "npx loki approve",
        "test:e2e": "cypress open",
        "storybook": "start-storybook -p 6006 -c ./config/storybook",
        "build-storybook": "build-storybook -c ./config/storybook",
        "visual:report:json": "node scripts/generate-visual-json-report.js",
        "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "visual:report:report": "npm run visual:report:json && npm run visual:report:html",
        "prepare": "husky install",
        "generate:slice": "node ./scripts/createSlice/index.js",
        "postinstall": "node ./scripts/clear-cache.js",
        "remove-feature": "ts-node ./scripts/remove-features.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.22.5",
        "@babel/core": "^7.22.5",
        "@babel/plugin-transform-runtime": "^7.22.10",
        "@babel/plugin-transform-typescript": "^7.22.11",
        "@babel/preset-env": "^7.22.5",
        "@babel/preset-react": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
        "@storybook/addon-actions": "^6.4.19",
        "@storybook/addon-essentials": "^6.4.19",
        "@storybook/addon-interactions": "^6.4.19",
        "@storybook/addon-links": "^6.4.19",
        "@storybook/builder-webpack5": "^6.4.19",
        "@storybook/manager-webpack5": "^6.4.19",
        "@storybook/react": "^6.4.19",
        "@storybook/testing-library": "^0.0.9",
        "@svgr/webpack": "^8.0.1",
        "@testing-library/dom": "^9.3.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/circular-dependency-plugin": "^5.0.5",
        "@types/jest": "^29.5.2",
        "@types/node": "^17.0.21",
        "@types/react": "^18.2.12",
        "@types/react-dom": "^18.2.5",
        "@types/react-router-dom": "^5.3.3",
        "@types/webpack": "^5.28.0",
        "@types/webpack-bundle-analyzer": "^4.6.0",
        "@types/webpack-dev-server": "^4.7.2",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "@vitejs/plugin-react": "^4.0.4",
        "babel-loader": "^9.1.2",
        "circular-dependency-plugin": "^5.2.2",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "cypress": "^13.1.0",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-i18next": "^6.0.3",
        "eslint-plugin-kinzaz-test-plugin": "^0.0.2",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^8.0.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-html-reporters": "^3.1.4",
